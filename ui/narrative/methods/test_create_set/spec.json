        {
            "id": "output_object",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "default_values": [ "MyReadsSet" ],
            "field_type": "text",
            "text_options": {
                "is_output_name": true,
                "valid_ws_types": [ "KBaseSets.ReadsSet" ]
            }
        },
        {
            "id": "description",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "default_values": [ "Reads Set Description" ],
            "field_type": "text",
            "text_options": {
                "is_output_name": false
            }
        },
        {
            "id": "input_reads_label",
            "optional": true,
            "field_type": "text",
            "allow_multiple": false
        },
        {
            "id": "input_reads_obj",
            "optional": true,
            "advanced": false,
            "allow_multiple": false,
            "default_values": [ "" ],
            "field_type": "text",
            "text_options": {
                "valid_ws_types": [ "KBaseFile.SingleEndLibrary", "KBaseFile.PairedEndLibrary" ]
            }
        },
        {
            "id": "input_reads_metadata",
            "optional": true,
            "field_type": "mapping"
        }
    ],
    "parameter-groups": [
        {
